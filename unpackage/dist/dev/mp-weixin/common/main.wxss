    left: 50px;
}
.circle-line text{
    display: block;
    width: 50%;
    height: 4rpx;
    opacity: .7;
    position: absolute;
    left: 25%;
    top: 50%;
    -webkit-transform-origin: center center;
            transform-origin: center center;
    -webkit-animation: circle 2.4s linear infinite;
            animation: circle 2.4s linear infinite;
}
.circle-line text::before{
    content: '';
    display: block;
    /* 调整加载的块的大小 */
    width: 12rpx;
    height: 4rpx;
    -webkit-border-radius: 1px;
            border-radius: 1px;
    position: absolute;
    top: 0;
    right: 10px;
    background-color: #999;
}
