<view class="contact pagination"><block wx:if="{{showAround&&!numAround}}"><view data-event-opts="{{[['tap',[['clickStart',['$event']]]]]}}" class="{{['pag-btn start',nowPage==1?'btn-ban':'']}}" bindtap="__e"><block wx:if="{{btnText}}"><text>首页</text></block><block wx:else><text class="icon icon-left1"></text></block></view></block><view data-event-opts="{{[['tap',[['clickPrev',['$event']]]]]}}" class="{{['pag-btn prev',nowPage==1?'btn-ban':'']}}" bindtap="__e"><block wx:if="{{btnText}}"><text>上页</text></block><block wx:else><text class="icon icon-left"></text></block></view><block wx:if="{{mode=='simple'}}"><view class="page-num simple">{{nowPage+" / "+pageNum}}</view></block><block wx:else><view class="page-num"><block wx:if="{{pageNum<=5}}"><block wx:for="{{pageNum}}" wx:for-item="page" wx:for-index="__i0__" wx:key="*this"><view data-event-opts="{{[['tap',[['clickPage',['$0'],[[['pageNum','',__i0__]]]]]]]}}" class="{{['pag-btn',page==nowPage?'active':'']}}" bindtap="__e">{{page}}</view></block></block><block wx:else><block wx:if="{{numAround}}"><view data-event-opts="{{[['tap',[['clickPage',[1]]]]]}}" class="{{['pag-btn',nowPage==1?'active':'']}}" bindtap="__e">1</view></block><view hidden="{{!(forceEllipses&&getFirstPage!=1||numAround&&getFirstPage!=2)}}" class="pag-btn"><text class="icon icon-more"></text></view><view data-event-opts="{{[['tap',[['clickPage',['$0'],['getFirstPage']]]]]}}" class="{{['pag-btn',getFirstPage==nowPage?'active':'']}}" bindtap="__e">{{getFirstPage}}</view><view data-event-opts="{{[['tap',[['clickPage',[getFirstPage+1]]]]]}}" class="{{['pag-btn',getFirstPage+1==nowPage?'active':'']}}" bindtap="__e">{{getFirstPage+1}}</view><view data-event-opts="{{[['tap',[['clickPage',[getFirstPage+2]]]]]}}" class="{{['pag-btn',getFirstPage+2==nowPage?'active':'']}}" bindtap="__e">{{getFirstPage+2}}</view><view data-event-opts="{{[['tap',[['clickPage',[getFirstPage+3]]]]]}}" hidden="{{!(!forceEllipses&&!numAround)}}" class="{{['pag-btn',getFirstPage+3==nowPage?'active':'']}}" bindtap="__e">{{getFirstPage+3}}</view><view data-event-opts="{{[['tap',[['clickPage',[getFirstPage+4]]]]]}}" hidden="{{!(!forceEllipses&&!numAround)}}" class="{{['pag-btn',getFirstPage+4==nowPage?'active':'']}}" bindtap="__e">{{getFirstPage+4}}</view><view hidden="{{!(forceEllipses&&getFirstPage<pageNum-2||numAround&&getFirstPage<pageNum-3)}}" class="pag-btn"><text class="icon icon-more"></text></view><block wx:if="{{numAround}}"><view data-event-opts="{{[['tap',[['clickPage',['$0'],['pageNum']]]]]}}" hidden="{{!(getFirstPage<pageNum-2)}}" class="{{['pag-btn',nowPage==pageNum?'active':'']}}" bindtap="__e">{{pageNum}}</view></block></block></view></block><view data-event-opts="{{[['tap',[['clickNext',['$event']]]]]}}" class="{{['pag-btn next',nowPage==pageNum?'btn-ban':'']}}" bindtap="__e"><block wx:if="{{btnText}}"><text>下页</text></block><block wx:else><text class="icon icon-right"></text></block></view><block wx:if="{{showAround&&!numAround}}"><view data-event-opts="{{[['tap',[['clickEnd',['$event']]]]]}}" class="{{['pag-btn end',nowPage==pageNum?'btn-ban':'']}}" bindtap="__e"><block wx:if="{{btnText}}"><text>尾页</text></block><block wx:else><text class="icon icon-right1"></text></block></view></block></view>