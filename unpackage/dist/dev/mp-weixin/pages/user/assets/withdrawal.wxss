	border-radius: 20rpx;
}
.bank-icon {
	width: 60rpx;
	height: 62rpx;
}
.bank-card-info {
	width: 400rpx;
	font-size: 32rpx;
	color: #303133;
	line-height: 44rpx;
	font-weight: bold;
}
