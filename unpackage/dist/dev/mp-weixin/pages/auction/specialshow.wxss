/* 列表轮播 */
uni-swiper-item {
	overflow: unset;
}
.swiper-item {
	height: 100%;
}
.list-trans {
	background-color: rgba(246, 246, 246, 1);
	position: absolute;
	width: 300%;
	left: -100%;
	top: 0;
	z-index: 9;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.list-transleft {
	-webkit-animation: trans180 0.6s infinite ease;
	        animation: trans180 0.6s infinite ease;
}
.list-transright {
	-webkit-animation: trans0 0.6s infinite ease;
	        animation: trans0 0.6s infinite ease;
}
@-webkit-keyframes trans180 {
0% {
		-webkit-transform: translateX(33%);
		        transform: translateX(33%);
}
100% {
		-webkit-transform: translateX(0%);
		        transform: translateX(0%);
}
}
@keyframes trans180 {
0% {
		-webkit-transform: translateX(33%);
		        transform: translateX(33%);
}
100% {
		-webkit-transform: translateX(0%);
		        transform: translateX(0%);
}
}
@-webkit-keyframes trans0 {
0% {
		-webkit-transform: translateX(0%);
		        transform: translateX(0%);
}
100% {
		-webkit-transform: translateX(33%);
		        transform: translateX(33%);
}
}
@keyframes trans0 {
0% {
		-webkit-transform: translateX(0%);
		        transform: translateX(0%);
}
100% {
		-webkit-transform: translateX(33%);
		        transform: translateX(33%);
}
}

